{
  "compilerOptions": {
    "strictPropertyInitialization": false,
    "target": "ES2023",
    "module": "NodeNext",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "outDir": "./dist",
    "declaration": true,
    "skipLibCheck": true,
    "paths": {
      "@commands/*": [
        "./src/commands/*"
      ],
      "@environments/*": [
        "./src/environments/*"
      ],
      "@events/*": [
        "./src/events/*"
      ],
      "@interactions/*": [
        "./src/interactions/*"
      ]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
